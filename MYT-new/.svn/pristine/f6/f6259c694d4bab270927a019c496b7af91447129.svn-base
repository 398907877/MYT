       <Sql name="queryCloseQueDoctorAndCount">
       SELECT
	*
FROM
	(
		SELECT
			ask_doctorid,
			COUNT (ask_doctorid) AS doccount
		FROM
			(
				SELECT
					*
				FROM
					(
						SELECT
							t1.*, t2.QD_SourceType,
							t2.QD_DocFreeID,
							t2.ASK_DoctorID
						FROM
							(
								SELECT
									quesmainid
								FROM
									ZIXUN_CloseQue
								WHERE
									1 = 1 @such
								GROUP BY
									quesmainid
							) AS t1
						LEFT JOIN ZiXun_QuesMain AS t2 ON t1.quesmainid = t2.quesmain_id
					) AS tt
				WHERE
					QD_SourceType = 1
				OR (
					QD_SourceType = 0
					AND QD_DocFreeID > 0
				)
			) AS yy
		GROUP BY
			ask_doctorid
	) AS yu
WHERE
	doccount = 150
       </Sql>
       
       
       

     <Sql name="queryListEveryDay">
     SELECT
	CASE
WHEN bbq.userdate IS NOT NULL THEN
	bbq.userdate
ELSE
	bbw.docdate
END datetime,
 bbq.usertopay,
 bbq.usertofree,
 bbq.userpub,
 bbw.doctopay,
 bbw.doctofree,
 bbw.docpub
FROM
	(
		SELECT
			CASE
		WHEN j1.datenow1 IS NOT NULL THEN
			j1.datenow1
		WHEN j2.datenow2 IS NOT NULL THEN
			j2.datenow2
		ELSE
			j3.datenow3
		END userdate,
		j1.usertopay,
		j2.usertofree,
		j3.userpub
	FROM
		(
			SELECT
				t1.datenow AS datenow1,
				COUNT (t1.datenow) AS usertopay
			FROM
				(
					SELECT
						CONVERT (
							VARCHAR (10),
							QUESMAIN_CreateTime,
							102
						) AS datenow
					FROM
						ZiXun_QuesMain
					WHERE
						1 = 1
					AND QD_DocFreeID != 0
					AND QD_SourceType = 0 @usersuch1
				) AS t1
			GROUP BY
				datenow
		) AS j1
	FULL OUTER JOIN (
		SELECT
			t1.datenow AS datenow2,
			COUNT (t1.datenow) AS usertofree
		FROM
			(
				SELECT
					CONVERT (
						VARCHAR (10),
						QUESMAIN_CreateTime,
						102
					) AS datenow
				FROM
					ZiXun_QuesMain
				WHERE
					1 = 1
				AND QD_DocFreeID = 0
				AND QD_SourceType = 0 @usersuch2
			) AS t1
		GROUP BY
			datenow
	) AS j2 ON j1.datenow1 = j2.datenow2
	FULL OUTER JOIN (
		SELECT
			t1.datenow AS datenow3,
			COUNT (t1.datenow) AS userpub
		FROM
			(
				SELECT
					CONVERT (
						VARCHAR (10),
						QUESMAIN_CreateTime,
						102
					) AS datenow
				FROM
					ZiXun_QuesMain
				WHERE
					1 = 1
				AND QD_SourceType = 1 @usersuch3
			) AS t1
		GROUP BY
			datenow
	) AS j3 ON j1.datenow1 = j3.datenow3
	) AS bbq
FULL OUTER JOIN (
	SELECT
		CASE
	WHEN p1.doctime1 IS NOT NULL THEN
		p1.doctime1
	WHEN p2.doctime2 IS NOT NULL THEN
		p2.doctime2
	ELSE
		p3.doctime3
	END docdate,
	p1.doctopay,
	p2.doctofree,
	p3.docpub
FROM
	(
		SELECT
			doctime AS doctime1,
			COUNT (doctime) AS doctopay
		FROM
			(
				SELECT
					CONVERT (
						VARCHAR (10),
						firstReplyTime,
						102
					) AS doctime
				FROM
					(
						SELECT
							*
						FROM
							(
								SELECT
									*
								FROM
									(
										SELECT
											MIN (REPLY_CreateTime) firstReplyTime,
											ASK_QuesID
										FROM
											dbo.ZiXun_reply
										WHERE
											REPLY_UserType = 1
										GROUP BY
											ASK_QuesID
									) AS oo
								WHERE
									1 = 1 @docsuch1
							) AS o1
						LEFT JOIN ZiXun_QuesMain AS o2 ON o1.ASK_QuesID = o2.QUESMAIN_ID
					) AS oo1
				WHERE
					1 = 1
				AND oo1.QD_SourceType = 0
				AND oo1.QD_DocFreeID != 0
			) AS k1
		GROUP BY
			k1.doctime
	) AS p1
FULL OUTER JOIN (
	SELECT
		doctime AS doctime2,
		COUNT (doctime) AS doctofree
	FROM
		(
			SELECT
				CONVERT (
					VARCHAR (10),
					firstReplyTime,
					102
				) AS doctime
			FROM
				(
					SELECT
						*
					FROM
						(
							SELECT
								*
							FROM
								(
									SELECT
										MIN (REPLY_CreateTime) firstReplyTime,
										ASK_QuesID
									FROM
										dbo.ZiXun_reply
									WHERE
										REPLY_UserType = 1
									GROUP BY
										ASK_QuesID
								) AS oo
							WHERE
								1 = 1 @docsuch2
						) AS o1
					LEFT JOIN ZiXun_QuesMain AS o2 ON o1.ASK_QuesID = o2.QUESMAIN_ID
				) AS oo1
			WHERE
				1 = 1
			AND oo1.QD_SourceType = 0
			AND oo1.QD_DocFreeID = 0
		) AS k1
	GROUP BY
		k1.doctime
) AS p2 ON p1.doctime1 = p2.doctime2
FULL OUTER JOIN (
	SELECT
		doctime AS doctime3,
		COUNT (doctime) AS docpub
	FROM
		(
			SELECT
				CONVERT (
					VARCHAR (10),
					firstReplyTime,
					102
				) AS doctime
			FROM
				(
					SELECT
						*
					FROM
						(
							SELECT
								*
							FROM
								(
									SELECT
										MIN (REPLY_CreateTime) firstReplyTime,
										ASK_QuesID
									FROM
										dbo.ZiXun_reply
									WHERE
										REPLY_UserType = 1
									GROUP BY
										ASK_QuesID
								) AS oo
							WHERE
								1 = 1 @docsuch3
						) AS o1
					LEFT JOIN ZiXun_QuesMain AS o2 ON o1.ASK_QuesID = o2.QUESMAIN_ID
				) AS oo1
			WHERE
				1 = 1
			AND oo1.QD_SourceType = 1
		) AS k1
	GROUP BY
		k1.doctime
) AS p3 ON p1.doctime1 = p3.doctime3
) AS bbw ON bbq.userdate = bbw.docdate
     
     </Sql>
     
     <Sql name="suchOneDateChannel">
     

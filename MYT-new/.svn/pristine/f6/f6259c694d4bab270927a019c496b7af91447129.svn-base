    <Sql name="queryNewDoctorMessage">
		SELECT
	t2.provinceid,
	t2.provincename,
	t2.cityid,
	t2.cityname,
	t2.hospitalid,
	t2.hosname,
	t2.hosdeptid,
	t2.deptname,
	t2.doctorname,
	t1.*
FROM
	(




		SELECT
			popop.onlyid,
			SUM (popop.toallcount) AS toallcount,
			SUM (popop.tobackcount) AS tobackcount,
			SUM (popop.tomin) AS tomin,
			SUM (popop.puballcount) as puballcount,
			SUM (popop.pubmin)as pubmin
		FROM
			(
				SELECT
					CASE
				WHEN oo.docidone IS NOT NULL THEN
					docidone
				WHEN oo.docidtwo IS NOT NULL THEN
					oo.docidtwo
				WHEN ll.idone IS NOT NULL THEN
					ll.idone
				ELSE
					ll.idtwo
				END onlyid,
				ll.numone AS toallcount,
				ll.backcount AS tobackcount,
				oo.timeone AS tomin,
				ll.numtwo AS puballcount,
				oo.timetwo AS pubmin
			FROM
				(
					SELECT
						a.ASK_DoctorID AS docidone,
						a.docRplyTime AS timeone,
						b.ASK_DoctorID AS docidtwo,
						b.docRplyTime AS timetwo
					FROM
						(
							SELECT
								ASK_DoctorID,
								SUM (minTime) / COUNT (ASK_DoctorID) AS docRplyTime
							FROM
								(
									SELECT
										ASK_DoctorID,
										DATEDIFF(
											mi,
											QUESMAIN_CreateTime,
											firstReplyTime
										) AS minTime,
										q1.QD_SourceType
									FROM
										(
											SELECT
												firstReplyTime,
												ASK_QuesID
											FROM
												(
													SELECT
														MIN (REPLY_CreateTime) firstReplyTime,
														ASK_QuesID
													FROM
														dbo.ZiXun_reply
													WHERE
														REPLY_UserType = 1
													GROUP BY
														ASK_QuesID
												) AS rp1
											WHERE
											
											   @minqueryone
												
										) AS t2
									LEFT JOIN dbo.ZiXun_QuesMain AS q1 ON q1.QUESMAIN_ID = t2.ASK_QuesID
									WHERE
										QD_SourceType = 0
								) AS t3
							GROUP BY
								ASK_DoctorID
						) AS a
					FULL OUTER JOIN (
						SELECT
							ASK_DoctorID,
							SUM (minTime) / COUNT (ASK_DoctorID) AS docRplyTime
						FROM
							(
								SELECT
									ASK_DoctorID,
									DATEDIFF(
										mi,
										QUESMAIN_CreateTime,
										firstReplyTime
									) AS minTime,
									q1.QD_SourceType
								FROM
									(
										SELECT
											firstReplyTime,
											ASK_QuesID
										FROM
											(
												SELECT
													MIN (REPLY_CreateTime) firstReplyTime,
													ASK_QuesID
												FROM
													dbo.ZiXun_reply
												WHERE
													REPLY_UserType = 1
												GROUP BY
													ASK_QuesID
											) AS rp1
										WHERE
										
											  @minquerytwo
											
									) AS t2
								LEFT JOIN dbo.ZiXun_QuesMain AS q1 ON q1.QUESMAIN_ID = t2.ASK_QuesID
								WHERE
									QD_SourceType = 1
								AND q1.ASK_DoctorID != '710003631'
							) AS t3
						GROUP BY
							ASK_DoctorID
					) AS b ON a.ASK_DoctorID = b.ASK_DoctorID
				) AS oo
			FULL OUTER JOIN (
				SELECT
					m.ASK_DoctorID AS idone,
					m.num AS numone,
					m.backcount,
					n.ASK_DoctorID AS idtwo,
					n.num AS numtwo
				FROM
					(
						SELECT
							a.ASK_DoctorID,
							a.num,
							b.num AS backcount
						FROM
							(
								SELECT
									ASK_DoctorID,
									COUNT (ASK_DoctorID) AS num
								FROM
									ZiXun_QuesMain
								WHERE
									QD_SourceType = 0   @countone
								GROUP BY
									ASK_DoctorID
							) AS a
						FULL OUTER JOIN (
							SELECT
								ASK_DoctorID,
								COUNT (ASK_DoctorID) AS num
							FROM
								ZiXun_QuesMain
							WHERE
								QD_SourceType = 0
							AND QD_IsReplay = 1   @counttwo
							GROUP BY
								ASK_DoctorID
						) AS b ON a.ASK_DoctorID = b.ASK_DoctorID
					) m
				FULL OUTER JOIN (
					SELECT
						ASK_DoctorID,
						COUNT (ASK_DoctorID) AS num
					FROM
						ZiXun_QuesMain
					WHERE
						QD_SourceType = 1   @countthree
					GROUP BY
						ASK_DoctorID
				) n ON m.ASK_DoctorID = n.ASK_DoctorID
			) AS ll ON oo.docidone = ll.idone
			) AS popop
		GROUP BY
			popop.onlyid
	



) AS t1
LEFT JOIN (
	SELECT
		*
	FROM
		dbo.v_myt_doctorService
	WHERE
		Main = 1
) AS t2 ON t1.onlyid = t2.doctoruid
WHERE
	1 = 1 @place
	</Sql>
	
	
	<Sql name="queryNewDoctorMessageDept">
		
	select  * from 
(
SELECT
			standarddeptid,
			COUNT (toallcount) AS toallcount,
			COUNT (tobackcount) AS tobackcount,
			COUNT (tomin) AS tomin,
			COUNT (puballcount) AS puballcount,
			COUNT (pubmin) AS pubmin
FROM
	(
		SELECT
			t2.provinceid,
			t2.provincename,
			t2.cityid,
			t2.cityname,
			t2.hospitalid,
			t2.hosname,
			t2.standarddeptid,
			t2.doctorname,
			t1.*
		FROM
			(SELECT
	popop.onlyid AS onlyid,
	SUM (popop.toallcount) AS toallcount,
	SUM (popop.tobackcount) AS tobackcount,
	SUM (popop.tomin)as tomin,
	SUM (popop.puballcount)as  puballcount,
	SUM (popop.pubmin)as pubmin
FROM
	(
		SELECT
			CASE
		WHEN oo.docidone IS NOT NULL THEN
			docidone
		WHEN oo.docidtwo IS NOT NULL THEN
			oo.docidtwo
		WHEN ll.idone IS NOT NULL THEN
			ll.idone
		ELSE
			ll.idtwo
		END onlyid,
		ll.numone AS toallcount,
		ll.backcount AS tobackcount,
		oo.timeone AS tomin,
		ll.numtwo AS puballcount,
		oo.timetwo AS pubmin
	FROM
		(
			SELECT
				a.ASK_DoctorID AS docidone,
				a.docRplyTime AS timeone,
				b.ASK_DoctorID AS docidtwo,
				b.docRplyTime AS timetwo
			FROM
				(
					SELECT
						ASK_DoctorID,
						SUM (minTime) / COUNT (ASK_DoctorID) AS docRplyTime
					FROM
						(
							SELECT
								ASK_DoctorID,
								DATEDIFF(
									mi,
									QUESMAIN_CreateTime,
									firstReplyTime
								) AS minTime,
								q1.QD_SourceType
							FROM
								(
									SELECT
										firstReplyTime,
										ASK_QuesID
									FROM
										(
											SELECT
												MIN (REPLY_CreateTime) firstReplyTime,
												ASK_QuesID
											FROM
												dbo.ZiXun_reply
											WHERE
												REPLY_UserType = 1
											GROUP BY
												ASK_QuesID
										) AS rp1
									WHERE    1=1   @minqueryone
								) AS t2
							LEFT JOIN dbo.ZiXun_QuesMain AS q1 ON q1.QUESMAIN_ID = t2.ASK_QuesID
							WHERE
								QD_SourceType = 0
						) AS t3
					GROUP BY
						ASK_DoctorID
				) AS a
			FULL OUTER JOIN (
				SELECT
					ASK_DoctorID,
					SUM (minTime) / COUNT (ASK_DoctorID) AS docRplyTime
				FROM
					(
						SELECT
							ASK_DoctorID,
							DATEDIFF(
								mi,
								QUESMAIN_CreateTime,
								firstReplyTime
							) AS minTime,
							q1.QD_SourceType
						FROM
							(
								SELECT
									firstReplyTime,
									ASK_QuesID
								FROM
									(
										SELECT
											MIN (REPLY_CreateTime) firstReplyTime,
											ASK_QuesID
										FROM
											dbo.ZiXun_reply
										WHERE
											REPLY_UserType = 1
										GROUP BY
											ASK_QuesID
									) AS rp1
								WHERE   1=1   @minquerytwo
							) AS t2
						LEFT JOIN dbo.ZiXun_QuesMain AS q1 ON q1.QUESMAIN_ID = t2.ASK_QuesID
						WHERE
							QD_SourceType = 1
						AND q1.ASK_DoctorID != '710003631'
					) AS t3
				GROUP BY
					ASK_DoctorID
			) AS b ON a.ASK_DoctorID = b.ASK_DoctorID
		) AS oo
	FULL OUTER JOIN (
		SELECT
			m.ASK_DoctorID AS idone,
			m.num AS numone,
			m.backcount,
			n.ASK_DoctorID AS idtwo,
			n.num AS numtwo
		FROM
			(
				SELECT
					a.ASK_DoctorID,
					a.num,
					b.num AS backcount
				FROM
					(
						SELECT
							ASK_DoctorID,
							COUNT (ASK_DoctorID) AS num
						FROM
							ZiXun_QuesMain
						WHERE
							QD_SourceType = 0   @countone
						GROUP BY
							ASK_DoctorID
					) AS a
				FULL OUTER JOIN (
					SELECT
						ASK_DoctorID,
						COUNT (ASK_DoctorID) AS num
					FROM
						ZiXun_QuesMain
					WHERE
						QD_SourceType = 0
					AND QD_IsReplay = 1     @counttwo
					GROUP BY
						ASK_DoctorID
				) AS b ON a.ASK_DoctorID = b.ASK_DoctorID
			) m
		FULL OUTER JOIN (
			SELECT
				ASK_DoctorID,
				COUNT (ASK_DoctorID) AS num
			FROM
				ZiXun_QuesMain
			WHERE
				QD_SourceType = 1    @countthree
			GROUP BY
				ASK_DoctorID
		) n ON m.ASK_DoctorID = n.ASK_DoctorID
	) AS ll ON oo.docidone = ll.idone
	) AS popop
GROUP BY
	popop.onlyid) AS t1
		LEFT JOIN (
			SELECT
				*
			FROM
				dbo.v_myt_doctorService
			WHERE
				Main = 1
		) AS t2 ON t1.onlyid = t2.doctoruid
		WHERE
			1 = 1 
	) AS tttt   
  GROUP BY  
	tttt.standarddeptid

)as  xxxxx

where    1=1    @such        ORDER BY standarddeptid  desc


	

     


  
	

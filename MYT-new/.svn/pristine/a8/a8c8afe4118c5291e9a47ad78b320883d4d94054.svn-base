package com.yihu.myt.service.service.impl;import java.util.List;import com.coreframework.db.DB;import com.coreframework.db.JdbcConnection;import com.coreframework.db.Sql;import com.yihu.myt.enums.MyDatabaseEnum;import com.yihu.myt.enums.ZiXun_DoctorApplyInterimSqlNameEnum;import com.yihu.myt.util.StringUtil;import com.yihu.myt.vo.ZiXun_DoctorApplyInterimVo;import com.yihu.myt.service.service.IZiXun_DoctorApplyInterimService;
public class ZiXun_DoctorApplyInterimService implements IZiXun_DoctorApplyInterimService{
	/**	*获取列表记录数	*/	public Integer queryZiXun_DoctorApplyInterimCountByCondition(ZiXun_DoctorApplyInterimVo vo) throws Exception{		Sql sql = DB.me().createSql(ZiXun_DoctorApplyInterimSqlNameEnum.queryZiXun_DoctorApplyInterimCountByCondition);		StringBuilder condition = new StringBuilder();		sql.addVar("@condition", condition.toString());		Integer count = DB.me().queryForInteger(MyDatabaseEnum.YiHuNet2008, sql);		return count;	}
	/**	*获取列表	*/	public List<ZiXun_DoctorApplyInterimVo> queryZiXun_DoctorApplyInterimListByCondition(ZiXun_DoctorApplyInterimVo vo) throws Exception{		Sql sql = DB.me().createSql(ZiXun_DoctorApplyInterimSqlNameEnum.queryZiXun_DoctorApplyInterimListByCondition);		StringBuilder condition = new StringBuilder();		sql.addVar("@condition", condition.toString());		sql.addVar("@page", "");		List<ZiXun_DoctorApplyInterimVo> list = DB.me().queryForBeanList(MyDatabaseEnum.YiHuNet2008, sql,ZiXun_DoctorApplyInterimVo.class);		return list;	}
	/**	*添加	*/	public void insertZiXun_DoctorApplyInterim(ZiXun_DoctorApplyInterimVo vo) throws Exception{}
	/**	*修改	*/	public void updateZiXun_DoctorApplyInterimByCondition(ZiXun_DoctorApplyInterimVo vo,JdbcConnection conn) throws Exception{Sql sql = DB.me().createSql(ZiXun_DoctorApplyInterimSqlNameEnum.updateZiXun_DoctorApplyInterimByCondition);		if (vo == null  || StringUtil.isEmpty(vo.getASK_UserID()) || StringUtil.isEmpty(vo.getDAI_ID())) {			throw new Exception(" 不能为空或者 主键id 不能为空");		}		StringBuilder condition = new StringBuilder();		if (condition.length() == 0) {		throw new Exception("未有更新SQL被执行！");		} else {		condition.deleteCharAt(condition.length() - 1);		sql.addVar("@condition", condition.toString());		sql.addParamValue(vo.getASK_UserID());		sql.addParamValue(vo.getDAI_ID());		}		DB.me().update(conn, sql);		}
}
package com.yihu.myt.service.service.impl;import java.util.List;import com.coreframework.db.DB;import com.coreframework.db.JdbcConnection;import com.coreframework.db.Sql;import com.yihu.myt.enums.MyDatabaseEnum;import com.yihu.myt.enums.DoctorInviteSqlNameEnum;import com.yihu.myt.util.StringUtil;import com.yihu.myt.vo.DoctorInviteVo;import com.yihu.myt.service.service.IDoctorInviteService;
public class DoctorInviteService implements IDoctorInviteService{
	/**	*获取列表记录数	*/	public Integer queryDoctorInviteCountByCondition(DoctorInviteVo vo) throws Exception{		Sql sql = DB.me().createSql(DoctorInviteSqlNameEnum.queryDoctorInviteCountByCondition);		StringBuilder condition = new StringBuilder();		sql.addVar("@condition", condition.toString());		Integer count = DB.me().queryForInteger(MyDatabaseEnum.YiHuNet2008, sql);		return count;	}
	/**	*获取列表	*/	public List<DoctorInviteVo> queryDoctorInviteListByCondition(DoctorInviteVo vo) throws Exception{		Sql sql = DB.me().createSql(DoctorInviteSqlNameEnum.queryDoctorInviteListByCondition);		StringBuilder condition = new StringBuilder();		sql.addVar("@condition", condition.toString());		sql.addVar("@page", "");		List<DoctorInviteVo> list = DB.me().queryForBeanList(MyDatabaseEnum.YiHuNet2008, sql,DoctorInviteVo.class);		return list;	}
	/**	*添加	*/	public void insertDoctorInvite(DoctorInviteVo vo) throws Exception{}
	/**	*修改	*/	public void updateDoctorInviteByCondition(DoctorInviteVo vo,JdbcConnection conn) throws Exception{Sql sql = DB.me().createSql(DoctorInviteSqlNameEnum.updateDoctorInviteByCondition);		if (vo == null  || StringUtil.isEmpty(vo.getDI_ID())) {			throw new Exception(" 不能为空或者 主键id 不能为空");		}		StringBuilder condition = new StringBuilder();		if (condition.length() == 0) {		throw new Exception("未有更新SQL被执行！");		} else {		condition.deleteCharAt(condition.length() - 1);		sql.addVar("@condition", condition.toString());		sql.addParamValue(vo.getDI_ID());		}		DB.me().update(conn, sql);		}
}